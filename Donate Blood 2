#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {


    int a,w;
    float h;
    scanf("%d%d",&a,&w);
    scanf("%f",&h);
    if(a>=18 && w>=45 && h>=5.5)
    {
        printf("He or She able to donate blood\n");
    }
    else
    {
        printf("He or She unable to donate blood\n");
    }
    return 0;
}

   
